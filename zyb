local StarterGui = game:GetService("StarterGui")
local Players = game:GetService("Players")
local Whitelisted = {
    ["suedrr4"] = true,
    ["dt106101zz"] = true,
    ["jay04_23"] = true,
    ["Kadys5373"] = true,
 }

local function IsWhitelisted(player)
    return Whitelisted[player.Name] or false
end

local localPlayer = Players.LocalPlayer

local isLocalPlayerWhitelisted = IsWhitelisted(localPlayer)

if isLocalPlayerWhitelisted then
    StarterGui:SetCore("SendNotification", {
        Title = "白名单认证",
        Text = "玩家:"..localPlayer.Name.."，腳本由可軒和初音共同製作",
        Duration = 7,
    })
loadstring(game:HttpGet("https://raw.githubusercontent.com/kismiletest/-/refs/heads/main/zyy"))()
else
    localPlayer:Kick("请联系作者添加白名单")
end
